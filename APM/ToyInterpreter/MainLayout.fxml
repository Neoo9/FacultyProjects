<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<?import java.net.URL?>
<AnchorPane prefHeight="250.0" prefWidth="550.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="View.GUI.PrgListController" styleClass="pane">
    <AnchorPane prefHeight="220.0" prefWidth="550.0">
        <ListView fx:id="myPrgList" focusTraversable="false" layoutX="95.0" layoutY="37.0" prefHeight="300.0"
                  prefWidth="550.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                  AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
    </AnchorPane>
    <AnchorPane layoutY="220.0" prefHeight="30.0" prefWidth="550.0">
        <Button fx:id="runButton" focusTraversable="false" layoutX="169.0" layoutY="2.0" mnemonicParsing="false"
                prefHeight="35.0" prefWidth="550.0" text="Run" AnchorPane.bottomAnchor="0.0"
                AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <font>
                <Font name="Comic Sans MS"/>
            </font>
        </Button>
    </AnchorPane>
    <stylesheets>
        <URL value="@mainCSS.css" />
    </stylesheets>
</AnchorPane>

